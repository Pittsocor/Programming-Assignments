function loneSum(a,b,c){
	if (a != b || a != c || b != c) {
	  if (a != b && a != c && b != c) {
	  	return a + b + c;
	  }
		if (a == b && a !=c) {
			return c;
		}
		if (b == c && b != a) {
			return a;
		}
		if (a == c && a != b) {
			return b;
		}
	} else {
		return 0;
	}
}

var output = loneSum(3,2,3);
console.log(output);
