function findMinLengthOfThreeWords(word1, word2, word3) {
  // your code here
  var w1 = word1.length;
  var w2 = word2.length;
  var w3 = word3.length;
  
  if (w1 < w2 && w3){
  	return w1
  }
  if (w2 < w1 && w3){
  	return w2
  }
  else {
  	return w3
  }
}

var output = findMinLengthOfThreeWords('a', 'be', 'see');
console.log(output); // --> 1
