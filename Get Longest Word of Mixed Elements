function getLongestWordOfMixedElements(arr) {
  // your code here
  var arr_2 = [];
  var t = 0;
  var longestWord = ""
  for (i=0;i<arr.length;i++){
  	if (typeof arr[i] == "string"){
  		if (arr[i].length > t){
  			t = arr[i].length;
  			longestWord = arr[i];
  		}
  	}
  }
  return longestWord;
}

var output = getLongestWordOfMixedElements([3, 'word', 5, 'up', 3, 1]);
console.log(output); // --> 'word'
